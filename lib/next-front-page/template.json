{
  "switchTabEvery": 5000,
  "updateDataEvery": 60000,
  "tabs": [

{% for tab in tabs %}

  {% if tab == "markets" %}{
    "title": "Markets",
    "type": "breakpoints",
    "data": {
      "default": "",
      "S": "",
      "M": "",
      "L": "",
      "XL": "",
    }
  }{% elif tab == "regional" %}{
    "title": "Regional",
    "type": "simple",
    "data": ""
  }{% elif tab == "national" %}{
    "title": "National",
    "link": "",
    "type": "result",
    "data": {
      "leave": {{leave}},
      "remain": {{remain}},
      "numDecimalPlaces": 1,
      "note": "{{note}}"
    }
  }{% endif %}{% if not loop.last %},{% endif%}

{% endfor %}

  ]
}
